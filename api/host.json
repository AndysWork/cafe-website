{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "http": {
            "routePrefix": "api"
        }
    },
    "cors": {
        "allowedOrigins": [
            "http://localhost:4200",
            "https://*.azurewebsites.net",
            "https://*.azurestaticapps.net"
        ],
        "supportCredentials": true
    }
}